   "source": [
    "a = torch.randn(2, 4, 3)\n",
    "lu, p = torch.lu(a)\n",
    "print(lu.shape, p.shape)\n",
    "print(p)\n",
    "q, l, u = torch.lu_unpack(lu, p)\n",
    "print(q.shape, l.shape, u.shape)\n",
    "print((a - torch.matmul(torch.matmul(q, l), u)))"
